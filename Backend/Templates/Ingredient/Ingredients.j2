{% extends "Header.j2" %}
{% block body %}
	<title>
		Ingredients
	</title>
	<table>
		{% for ingredient in ingredients %}
			<tr>
				<td>
					<a
					  href="/ingredient/{{ ingredient.names()[0] }}"
					>
						{{ ingredient.names()[0] }}
					</a>
				</td>
			</tr>
		{% endfor %}
	</table>
{% endblock %}
